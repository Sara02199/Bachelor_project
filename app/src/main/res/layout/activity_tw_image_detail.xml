<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFDD0"
    android:padding="50dp"
    tools:context=".TwImageDetail">
    <ImageView
        android:id="@+id/ive"
        android:layout_width="331dp"
        android:layout_height="288dp"
        android:src="@drawable/ffffffpvff" />
    <TextView
        android:id="@+id/t8"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/ive"
        android:text="Parsley works as a powerful natural diuretic and can help reduce bloating and blood pressure. "
        android:textAlignment="center"
        android:textColor="#024A05"
        android:textSize="18dp"
        android:textStyle="bold" />
    <TextView
        android:id="@+id/tvew9"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/t8"
        android:text="0.300"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="18dp"
        android:textStyle="bold" />
    <Button
        android:id="@+id/add12"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvew9"
        android:backgroundTint="#d4b781"
        android:textColor="@color/black"
        android:text="Go back to view products" />
</RelativeLayout>